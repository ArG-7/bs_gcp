{ 
  "script":{
    "license":"Apache License, Version 2.0",
    "copyright":"Copyright 2018 Google Inc.",
    "open":true,
    "icon":"file_download",
    "product":"3P Data",
    "title":"IAS Storage",
    "description":"Import emailed IAS report into a Storage bucket.",
    "instructions":[
      "The person executing this recipe must be the recipient of the email.",
      "Schedule a IAS report to be sent to <b>{{ EMAIL_TOKEN }}</b>.",
      "Give a regular expression to match the email link or attachment.",
      "THe report will be added to the storage bucket."
    ],
    "authors":["kenjora@google.com"]
  },
  "ias":{
    "auth":"user",
    "in":{
      "email":{
        "days":3,
        "from":"reports@integralads.com",
        "to":{"constant":"email_token"},
        "link":{"field":{ "name":"ias_link", "kind":"string", "order":3, "default":"" }},
        "attachment":{"field":{ "name":"ias_attachment", "kind":"string", "order":4, "default":"" }}
      }
    },
    "out":{
      "storage":{
        "bucket":{"field":{ "name":"ias_bucket", "kind":"string", "order":6, "default":"" }},
        "path":{"field":{ "name":"ias_path", "kind":"string", "order":7, "default":"" }}
      }
    }
  }
}
