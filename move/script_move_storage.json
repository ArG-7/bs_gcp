{
  "script":{
    "icon":"arrow_forward",
    "product":"BigQuery",
    "title":"Storage To Table",
    "description":"Move data from a Storage bucket into a BigQuery table.",
    "authors":["kenjora@google.com"]
  },
  "move":{
    "auth":"service",
    "from":{
      "bucket":{"field":{ "name":"move_bucket", "kind":"string", "order":1, "default":"" }},
      "path":{"field":{ "name":"move_path", "kind":"string", "order":2, "default":"" }}
    },
    "to":{
      "dataset":{"field":{ "name":"move_dataset", "kind":"string", "order":3, "default":"" }},
      "table":{"field":{ "name":"move_table", "kind":"string", "order":4, "default":"" }}
    }
  }
}
