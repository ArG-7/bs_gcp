            "metrics": [
              "METRIC_MEDIA_COST_ADVERTISER",
              "METRIC_IMPRESSIONS",
              "METRIC_CLICKS",
              "METRIC_TOTAL_CONVERSIONS",
              "METRIC_LAST_CLICKS",
              "METRIC_LAST_IMPRESSIONS",
              "METRIC_CM_POST_CLICK_REVENUE",
              "METRIC_CM_POST_VIEW_REVENUE",
              "METRIC_REVENUE_ADVERTISER"
