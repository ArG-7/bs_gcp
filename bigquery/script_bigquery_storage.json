{
  "script":{
    "license":"Apache License, Version 2.0",
    "copyright":"Copyright 2018 Google Inc.",
    "open":true,
    "icon":"arrow_forward",
    "product":"BigQuery",
    "title":"Storage To Table",
    "description":"Move using bucket and path prefix.",
    "instructions":[
      "Specify a bucket and path prefix, * suffix is NOT required.",
      "Every time the job runs it will overwrite the table."
    ],
    "authors":["kenjora@google.com"]
  },
  "tasks":[
    { "bigquery":{
      "auth":"user",
      "from":{
        "bucket":{"field":{ "name":"bucket", "kind":"string", "order":1, "default":"" }},
        "path":{"field":{ "name":"path", "kind":"string", "order":2, "default":"" }}
      },
      "to":{
        "auth":"service",
        "dataset":{"field":{ "name":"dataset", "kind":"string", "order":3, "default":"" }},
        "table":{"field":{ "name":"table", "kind":"string", "order":4, "default":"" }}
      },
      "schema":{"field":{ "name":"schema", "kind":"json", "order":5, "default":"" }}
    }}
  ]
}
