{
  "script":{
    "license":"Apache License, Version 2.0",
    "copyright":"Copyright 2018 Google Inc.",
    "open":true,
    "icon":"file_download",
    "product":"DBM",
    "title":"Entity Read To BigQuery",
    "description":"Moves all entity read files into BigQuery.",
    "instructions":[
      "Specify a list of partner ids, comma seperated.",
      "Files are <a href='https://developers.google.com/bid-manager/guides/entity-read/overview'>imported from storage</a> using your user credentials.",
      "Both public and private files are imported.",
      "Each file gets a table in the dataset, prefixed with <strong>'Entity_'</strong>.",
      "CAUTION: Entity Read Files are ONLY partner level."
    ],
    "authors":["mauriciod@google.com", "pkenjora@google.com"]
  },
  "entity":{
    "auth":"user",
    "version":0.2,
    "prefix":"Entity",
    "accounts": {"field":{ "name":"partners", "kind":"integer_list", "order":1, "default":"[]" }},
    "entities":["LineItem", "Creative", "UserList", "Partner", "Advertiser", "InsertionOrder", "Pixel", "InventorySource", "CustomAffinity", "UniversalChannel", "UniversalSite", "SupportedExchange", "DataPartner", "GeoLocation", "Language", "DeviceCriteria", "Browser", "Isp"],
    "out":{
      "auth":"service",
      "dataset": {"field":{ "name":"entity_dataset", "kind":"string", "order":3, "default": "" }}
    }
  }
}
