{
  "script":{
    "license":"Apache License, Version 2.0",
    "copyright":"Copyright 2018 Google Inc.",
    "open":true,
    "icon":"donut_small",
    "product":"DCM",
    "title":"DCM Report",
    "description":"Create a DCM report.",
    "instructions":[
      "Reference field values from the <a href='https://developers.google.com/bid-manager/v1/reports'>DCM API</a> to build a report.",
      "Specify an account_id or account_id:subaccount_id.",
      "The report is only created, use a move script to move it.",
      "A report with a ( Starthinker ) suffix will be created in DCM.",
      "To reset a report, delete it from DCM reporting."
    ],
    "authors":["jfno@google.com", "kenjora@google.com"]
  },
  "dcm":{
    "auth":"user",
    "report":{
      "template":{"field":{ "name":"dcm_template", "kind":"choice", "order":2, "choices":["standard", "path_to_conversion"], "default":"standard" }},
      "account_id":{"field":{ "name":"dcm_account", "kind":"string", "order":3, "default":"" }},
      "name":{"field":{ "name":"dcm_name", "kind":"string", "order":4, "default":"" }},
      "format":"CSV",
      "relativeDateRange":{"field":{ "name":"dcm_range", "kind":"string", "order":6, "default":"" }},
      "dimensions":{
        "field":{
          "name":"dcm_dimensions",
          "kind":"string_list",
          "order":7,
          "default":[
            "date",
            "platformType",
            "creativeType",
            "state",
            "dmaRegion"
          ]
        }
      },
      "metrics":{
        "field":{
          "name":"dcm_metrics",
          "kind":"string_list",
          "order":8,
          "default":["impressions"]
        }
      }
    },
    "delete":{"field":{ "name":"dcm_delete", "kind":"boolean", "order":11, "default":"" }}
  }
}
