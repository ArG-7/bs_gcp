{
  "script":{
    "license":"Apache License, Version 2.0",
    "copyright":"Copyright 2018 Google Inc.",
    "icon":"assignment_returned",
    "product":"DCM",
    "title":"DT To Table",
    "description":"Move data from a DT bucket into a BigQuery table.",
    "instructions":[
      "Ensure your user has <a href='https://developers.google.com/doubleclick-advertisers/dtv2/getting-started' target='_blank'>access to the bucket</a>.",
      "Provide the DT bucket name to read from.",
      "Provide the path of the files to read."
    ],
    "authors":["kenjora@google.com"]
  },
  "dt":{
    "auth":"user",
    "from":{
      "bucket":{"field":{ "name":"bucket", "kind":"string", "order":1, "default":"" }},
      "path":{"field":{ "name":"path", "kind":"string", "order":2, "default":"" }}
    },
    "to":{
      "auth":"service",
      "dataset":{"field":{ "name":"dataset", "kind":"string", "order":3, "default":"" }},
      "table":{"field":{ "name":"table", "kind":"string", "order":4, "default":"" }}
    }
  }
}
